{% if user.avatar is defined and user.avatar %}
	<img class="img_avatar" src="{{ url_asset('avatar', user.id, user.avatar) }}" alt="avatar"/>
{% else %}
	{% image '@WittyUserBundle/Resources/public/images/avatar_118_default.jpg' %}
		<img class="img_avatar" src="{{ asset_url }}"/>
	{% endimage %}
{% endif %}